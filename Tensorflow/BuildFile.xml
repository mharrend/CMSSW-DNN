<use name="python" />
<use name="py2-numpy" />
<use name="DNN/Base" />
<Flags CXXFLAGS="-g -O0" />
<export>
    <lib name="1" />
</export>



<tool name="py2-numpy" version="1.11.1-ikhhed2">
  <client>
    <environment name="PY2_NUMPY_BASE" default="/cvmfs/cms.cern.ch/slc6_amd64_gcc530/external/py2-numpy/1.11.1-ikhhed2"/>
    <environment name="LIBDIR" default="$PY2_NUMPY_BASE/lib/python2.7/site-packages/numpy-1.11.1-py2.7-linux-x86_64.egg/numpy/core/lib"/>
    <environment name="INCLUDE" default="$PY2_NUMPY_BASE/lib/python2.7/site-packages/numpy-1.11.1-py2.7-linux-x86_64.egg/numpy/core/include"/>
  </client>
  <runtime name="PATH" value="$PY2_NUMPY_BASE/bin" type="path"/>
  <runtime name="ROOT_INCLUDE_PATH" value="$INCLUDE" type="path"/>
  <runtime name="PYTHONPATH" value="$PY2_NUMPY_BASE/lib/python2.7/site-packages" type="path"/>
  <use name="python"/>
  <use name="zlib"/>
  <use name="lapack"/>
</tool>
